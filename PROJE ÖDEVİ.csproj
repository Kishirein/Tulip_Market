    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Red Delicious Apples Didn_t Always Suck_ Here_s What Happened_.jpg" />
  </ItemGroup>

  <ItemGroup>
